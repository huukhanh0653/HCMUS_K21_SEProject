type Query {
    findEarlier(serverId: String!, channelId: String!, offset: Int!, limit: Int!): [Message]
    findCache(serverId: String!, channelId: String!, offset: Int!, limit: Int!): [Message]
}

type Message {
    messageId: String
    senderId: String
    serverId: String
    channelId: String
    timestamp: String
    content: MessageContent
    attachments: [String]
}

type MessageContent {
    text: String
}
